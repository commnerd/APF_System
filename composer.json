{
    "name": "commnerd/apf_system",
    "description": "Antiquated PHP Framework System Components designed to work with PHP ~5.3",
    "keywords": ["framework", "apf"],
    "type": "library",
    "require": {
        "altorouter/altorouter" : "1.1.0",
        "phpseclib/phpseclib" : "2.0.0",
        "ircmaxell/random-lib" : "1.0",
        "codeguy/upload" : "1.3.2",
        "twig/twig" : "~1.0",
        "phpoffice/phpexcel" : "1.8.1"
    },
    "autoload" : {
        "psr-4" : {
                "System\\" : "System/"
        }
    },
    "config": {
        "preferred-install": "dev",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
